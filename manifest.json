{
    "manifest_version": 3,
    "name": "QuipIn",
    "description": "Your personal, smart and witty reply assistant",
    "version": "0.1.0",
    "content_scripts": [
        {
            "matches": ["https://www.linkedin.com/*"],
            "js": ["content.js"]
        }
    ],
    "permissions": [
        "storage"
    ],
    "action": {
        "default_popup": "options.html",
        "default_icon": "icon.png"
    }
}